    color: #A9A9A9; /* Texto desbotado */
}

QComboBox::drop-down {
    width: 20px;
	border: none;
	background-color: #FAFAFA;
}

QComboBox::drop-down:disabled {
	background-color: #D3D3D3; /* Fundo cinza */
    color: #A9A9A9; /* Texto desbotado */
}

QComboBox::down-arrow {
