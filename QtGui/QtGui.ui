    color: #000;
    padding: 8px 12px;
    border: 1px solid #B1B1B1;
    border-bottom: none; /*Remove a borda inferior das abas */
}

QTabBar::tab:selected {
    background-color: #FAFAFA; /* Fundo da aba selecionada */
	color: #000;
}

QTabWidget::pane {
    border: 1px solid #B1B1B1; /* Borda ao redor do conteúdo */
    top: -1px; /* Sobe o conteúdo para &quot;colar&quot; na aba */
}

QLineEdit {
	border: 1px solid #B1B1B1;
	border-radius: 20px;
	color: #0000;
	background-color: #FAFAFA;
	padding: 2px 4px 2px 4px;
}

QLineEdit:disabled {
    background-color: #D3D3D3; /* Fundo cinza */
    color: #A9A9A9; /* Texto desbotado */
}

QComboBox {
	border: 1px solid #B1B1B1;
	border-radius: 20px;
	color: #000;
	backgroundColor: #FAFAFA;
	padding: 2px 4px 2px 4px;
}

QComboBox:disabled {
